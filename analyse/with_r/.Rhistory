scale_x_continuous(breaks = seq(1,10,1), labels = c("5","10","15","20","21","25","30","35","40","42")) +
scale_y_log10() +
labs(y="Geschwindikeit (in m/s)", x="Kilometerabschnitt", title = paste("Pace in ",o," (",g,"): TOP-",p, sep="")) +
#theme(legend.position = "none") +
facet_wrap(~Jahr)
}
print(plot_pace)
}
}
}
dev.off()
}
